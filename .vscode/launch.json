{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome localhost:9009",
      "url": "http://localhost:9009?e2e=true",
      "runtimeArgs": ["--load-extension=${workspaceFolder}/dist"]
    },
    {
      "name": "Jest Test local",
      "type": "node",
      "request": "launch",
      "program": "node_modules/jest/bin/jest.js",
      "args": [
        "--runInBand"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js",
        "!**/node_modules/**"
      ],
      "sourceMaps": true,
      "outputCapture": "std",
      "console": "internalConsole"
    }
  ]
}